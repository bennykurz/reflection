{
  "name": "n86io/reflection",
  "description": "Extends the PHP ReflectionClass, ReflectionMethod and ReflectionProperty with doc-comment parsing.",
  "license": "GPL-3.0+",
  "keywords": [
    "class",
    "method",
    "property",
    "reflection"
  ],
  "authors": [
    {
      "name": "Viktor Firus",
      "email": "viktor@neunzehn86.de"
    }
  ],
  "require": {
    "phpdocumentor/reflection-docblock": "^3.1",
    "phpdocumentor/type-resolver": "^0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "N86io\\Reflection\\": "src",
      "N86io\\Reflection\\Tests\\Stuff\\": "tests/Unit/Stuff"
    }
  }
}
